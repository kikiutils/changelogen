{
  "name": "@kikiutils/changelogen",
  "version": "0.8.0",
  "description": "Generate Beautiful Changelogs using Conventional Commits",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kikiutils/changelogen.git"
  },
  "contributors": [
    {
      "name": "unjs"
    },
    {
      "name": "kiki-kanri"
    }
  ],
  "license": "MIT",
  "sideEffects": false,
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.mts",
      "default": "./dist/index.mjs"
    }
  },
  "bin": {
    "changelogen": "./dist/cli.mjs"
  },
  "types": "./dist/index.d.mts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "unbuild",
    "changelogen": "jiti ./src/cli.ts --hideAuthorEmail",
    "dev": "vitest dev",
    "lint": "eslint . && prettier -c src test",
    "lint:fix": "eslint --fix . && prettier -w src test",
    "prepack": "unbuild",
    "release": "pnpm test && pnpm changelogen --release --push && pnpm publish",
    "test": "pnpm lint && vitest run --coverage"
  },
  "dependencies": {
    "c12": "^3.0.2",
    "confbox": "^0.2.1",
    "consola": "^3.4.0",
    "convert-gitmoji": "^0.1.5",
    "mri": "^1.2.0",
    "node-fetch-native": "^1.6.6",
    "ofetch": "^1.4.1",
    "open": "^10.1.0",
    "pathe": "^2.0.3",
    "pkg-types": "^2.1.0",
    "scule": "^1.3.0",
    "semver": "^7.7.1",
    "std-env": "^3.8.1"
  },
  "devDependencies": {
    "@types/node": "^22.13.10",
    "@types/semver": "^7.5.8",
    "@vitest/coverage-v8": "^3.0.8",
    "eslint": "^9.22.0",
    "eslint-config-unjs": "^0.4.2",
    "jiti": "^2.4.2",
    "prettier": "^3.5.3",
    "standard-version": "^9.5.0",
    "typescript": "^5.8.2",
    "unbuild": "^3.5.0",
    "vitest": "^3.0.8"
  },
  "packageManager": "pnpm@10.6.1",
  "pnpm": {
    "onlyBuiltDependencies": [
      "esbuild"
    ]
  }
}
